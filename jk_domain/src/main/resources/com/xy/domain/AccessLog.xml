<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.xy.domain">
	<class name="AccessLog" table="access_log_p">
		<id name="id" column="ACCESS_LOG_ID">
			<generator class="uuid"/>
		</id>
		<property name="userId" column="USER_ID"/>
		<property name="moduleKey" column="MODULE_KEY"/>
		<property name="moduleName" column="MODULE_NAME"/>
		<property name="moduleCurl" column="MODULE_CURL"/>
		<property name="number" column="NUMBER"/>
		<property name="ico" column="ICO"/>

		<property name="createBy" column="CREATE_BY"/>
		<property name="createTime" column="CREATE_TIME"/>
		
	</class>
</hibernate-mapping>